<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hci_g8.restaurantfinder" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
    >

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".NewGroup"></activity>

        <activity android:name=".SavedGroups"></activity>

        <activity android:name=".EditProfile"></activity>

        <activity android:name=".Results"></activity>
        <activity android:name=".Result1"></activity>
        <activity android:name=".Result2"></activity>
        <activity android:name=".Result3"></activity>
        <activity android:name=".Result4"></activity>

    </application>

</manifest>
